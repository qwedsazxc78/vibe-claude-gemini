# AI 協作配置文件

# 模型配置
CLAUDE_MODEL="sonnet"
GEMINI_MODEL="gemini-2.5-flash"

# 輸出模式
VERBOSE_MODE=false
LOG_PROMPTS=false
STREAM_OUTPUT=false

# 測試配置
RUN_TYPESCRIPT_CHECK=true
RUN_ESLINT_CHECK=true
RUN_UNIT_TESTS=true

# 目錄配置
REVIEWS_DIR="reviews"
LOGS_DIR="logs"
REPORTS_DIR="reports"
STATE_DIR=".collaboration-state"

# 提示詞長度限制
MAX_PROMPT_LENGTH=1000
ENABLE_PROMPT_COMPRESSION=true

# 並行處理
ENABLE_PARALLEL_REVIEW=false
MAX_CONCURRENT_TASKS=2